package builder;

public class CarBuilder  implements BuilderInterface {

	private Car car ;
	public CarBuilder() {
		this.reset();
	}
	public void reset() {
		this.car = new Car();
	}

	@Override
	public void setSeats(int nbrSeats) {
		this.car.setSeats(nbrSeats);
	}

	@Override
	public void setMat(int mat) {
		this.setMat(mat);
	}

	@Override
	public void setGps(boolean gps) {
		this.setGps(gps);
		
	}

	@Override
	public void getObject() {
		
	}

}
